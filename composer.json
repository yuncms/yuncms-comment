{
  "name": "yuncms/yuncms-comment",
  "description": "The comment module for yuncms.",
  "keywords": [
    "yuncms",
    "comment"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "yiisoft/yii2": "~2.0.0",
    "yuncms/yuncms-user": "~2.0",
    "yuncms/yuncms-core": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\comment\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "id": "comment",
      "translate": {
        "class": "yii\\i18n\\PhpMessageSource",
        "sourceLanguage": "en-US",
        "basePath": "@yuncms/comment/messages"
      },
      "migrationNamespace": "yuncms\\comment\\migrations",
      "backend": {
        "class": "yuncms\\comment\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\comment\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
